<h1>New karma</h1>

<% form_for(@karma) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :pkg %><br />
    <%= f.text_field :pkg %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :version %><br />
    <%= f.text_field :version %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', karmas_path %>
